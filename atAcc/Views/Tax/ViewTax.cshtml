<div class="page-wrapper">
    <!-- ============================================================== -->
    <!-- Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- End Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Container fluid  -->
    <!-- ============================================================== -->
    <div class="container-fluid">
        <!-- ============================================================== -->
        <!-- Start Page Content -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="card card-outline-info">
                    <div class="card-header">
                        <h4 class="m-b-0 text-white">Tax View</h4>
                    </div>
                    <div class="card-body">
                        <form role="form" method="get" action="#">

                            <div class="row pl-2 pt-2">
                                <form role="form" method="get" action="#">
                                    <div class="table-responsive" style="height:300px; overflow-y:auto; width:60%; ">
                                        <table id="mainTable" class="table editable-table table-bordered table-striped m-b-0" ">
                                            <thead>
                                                <tr>
                                                    <th>Sl No</th>
                                                    <th>Tax Name</th>
                                                    <th>Tax Percentage</th>
                                                    <th>Actions</th>
                                                </tr>
                                            </thead>
                                            <tbody>

                                                @foreach (var taxes in ViewBag.data)
                                                {
                                                    
                                                        <tr>
                                                            <td>@taxes.id</td>
                                                            <td>@taxes.tax_name</td>
                                                            <td>@taxes.taxprcnt %</td>
                                                            <td align="center"><a href="" data-toggle="tooltip" title="Edit"><i class="fas fa-user-edit"></i></a> &nbsp; <a href="" data-toggle="tooltip" title="Delete"><i class="fas fa-trash"></i></a></td>
                                                        </tr>
                                                    
                                                }
                                            </tbody>
                                        </table>
                                    </div>
                                </form>
                            </div>
                            <div class="row">
                                <div class="col">
                                    <a href="/Rack/Index" class="btn btn-primary">Add Rack</a>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- row -->
